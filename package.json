{
  "name": "greeneetwork",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Danilo JR3 Panaro",
    "email": "danilo.panaro@jr3.it",
    "url": "https://www.jr3.it"
  },
  "description": "NextJS App built with TypeScript, Jest, Enzyme, Storybook, Prettier, ESLint",
  "license": "MIT",
  "homepage": "https://gitlab.com/agritettura/greeneetwork#readme",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "yarn build:sitemap && next build",
    "build:sitemap": "babel-node -x \".ts\" -r dotenv/config ./utils/index.ts",
    "dev": "next dev",
    "start": "next start",
    "export": "yarn && yarn build && next export",
    "pretty:quick": "pretty-quick --pattern '**/*.*(ts|tsx|jsx)' --staged",
    "pretty:write": "prettier \"{data,src,public,utils}/**/*.{ts,tsx,js,jsx,json,md,css,geojson}\" --write",
    "pretty:test": "prettier \"{data,src,public,utils}/**/*.{ts,tsx,js,jsx,json,md,css,geojson}\" --list-different",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "sb:test": "cd .storybook && jest storyShot.test.js && cd ..",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty:quick && git add .",
      "pre-push": "yarn type-check && yarn test --passWithNoTests"
    }
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.18.2",
    "@turf/centroid": "^6.0.2",
    "atomize": "^1.0.26",
    "d3-ease": "^1.0.6",
    "framer-motion": "^1.10.3",
    "next": "^9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-html-parser": "^2.0.2",
    "react-icons": "^3.10.0",
    "react-map-gl": "^5.2.7",
    "react-measure": "^2.3.0",
    "react-player": "^2.1.1",
    "react-progressive-image": "^0.6.0",
    "react-select": "^3.1.0",
    "react-share": "^4.2.0",
    "react-transition-group": "^4.3.0",
    "styletron-engine-atomic": "^1.4.6",
    "styletron-react": "^5.2.7",
    "supercluster": "^7.1.0",
    "use-supercluster": "^0.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@directus/sdk-js": "^6.1.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^25.2.2",
    "@types/mapbox-gl": "^1.10.2",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.35",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-map-gl": "^5.2.3",
    "@types/react-select": "^3.0.13",
    "@types/react-share": "^3.0.3",
    "@types/sharp": "^0.25.0",
    "@types/styletron-engine-atomic": "^1.1.0",
    "@types/styletron-react": "^5.0.2",
    "@types/styletron-standard": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "@zeit/next-source-maps": "^0.0.3",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
